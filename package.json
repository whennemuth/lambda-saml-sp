{
  "name": "bu-lambda-shibboleth",
  "version": "0.1.0",
  "bin": {
    "bu-lambda-shibboleth": "bin/bu-lambda-shibboleth.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cleanup": "ts-node bin/LambdaVersionCleanup.ts",
    "test": "jest --silent",
    "cdk": "cdk",
    "sp-bundle": "esbuild ./lib/lambda/FunctionSpOrigin.ts --bundle --platform=node --outfile=sp.js",
    "bundle": "npm run sp-bundle && esbuild ./docker-entrypoint.js --bundle --platform=node --outfile=entrypoint.js",
    "install-all": "sh -c 'for line in $(find . -maxdepth 3 -name package.json -print | grep -v '/node_modules/') ; do (cd $(dirname $line) && npm install --save-dev); done;'",
    "docker-clean": "docker rmi $(docker images --filter dangling=true -q) 2> /dev/null || true",
    "up": "npm run bunde && npm run docker-clean && docker compose --profile separate up --build -d --remove-orphans",
    "down": "docker compose --profile separate down"
  },
  "dependencies": {
    "aws-cdk-lib": "2.99.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@aws-sdk/client-lambda": "^3.462.0",
    "@types/jest": "^29.5.5",
    "@types/node": "20.6.3",
    "@types/node-forge": "^1.3.8",
    "aws-cdk": "2.99.0",
    "aws-sdk-client-mock": "^3.0.0",
    "aws-sdk-client-mock-jest": "^3.0.0",
    "axios": "^1.6.3",
    "esbuild": "^0.19.5",
    "express": "^4.18.2",
    "https": "^1.0.0",
    "jest": "^29.7.0",
    "multer": "^1.4.5-lts.1",
    "node-forge": "^1.3.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2"
  }
}
